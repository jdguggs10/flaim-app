# ESPN Fantasy Baseball MCP Bridge - Environment Variables
# Copy this file to .env and fill in your ESPN credentials

# ESPN Fantasy Baseball Authentication
# Get these from browser cookies after logging into ESPN Fantasy
ESPN_S2=your_espn_s2_cookie_value_here
SWID=your_swid_cookie_value_here
LEAGUE_ID=your_league_id_here

# Optional: JWT Secret for authentication (generate a secure random string)
JWT_SECRET=your_jwt_secret_for_api_authentication

# Optional: Custom ports (defaults shown)
APISIX_HTTP_PORT=9080
APISIX_ADMIN_PORT=9180
PROMETHEUS_PORT=9090